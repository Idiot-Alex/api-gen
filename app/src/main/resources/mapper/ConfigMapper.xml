<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hotstrip.runapi.domain.mapper.ConfigMapper">

    <select id="listPage" resultType="com.hotstrip.runapi.domain.model.Config">
        SELECT distinct *
        FROM PUBLIC.config
        WHERE 1=1
        <if test="info.name != null">
            AND url like concat('%', #{info.name}, '%')
        </if>
        order by create_time desc
    </select>
</mapper>